      <bind nodeset="Meta/DeviceID" jr:preload="property" jr:preloadParams="DeviceID" />
      <bind nodeset="Meta/username" jr:preload="meta" jr:preloadParams="UserName" />
      <bind nodeset="Meta/chw_id" type="xsd:string" jr:preload="meta" jr:preloadParams="UserID" />
      <bind nodeset="Meta/TimeStart" type="dateTime" jr:preload="timestamp" jr:preloadParams="start" />
      <bind nodeset="Meta/TimeEnd" type="dateTime" jr:preload="timestamp" jr:preloadParams="end" />
	  <bind nodeset="Meta/uid" type="xsd:string" jr:preload="uid" jr:preloadParams="general" />
	  <bind nodeset="Meta/CommCareVersion" jr:preload="meta" jr:preloadParams="AppVersion" />
	  	  
	    <!-- Case XML Binds -->
		
		<bind nodeset="/safe_pregnancy/case/case_id" type="xsd:string" jr:preload="uid" jr:preloadParams="general"/>
		<bind nodeset="/safe_pregnancy/case/date_modified" type="xsd:dateTime" jr:preload="timestamp" jr:preloadParams="end"/>
		<bind nodeset="/safe_pregnancy/case/create/user_id" calculate="/safe_pregnancy/Meta/chw_id"/>
		<bind nodeset="/safe_pregnancy/case/create/case_name" calculate="/safe_pregnancy/mother_name"/>
		<bind nodeset="/safe_pregnancy/case/create/external_id" type="xsd:string" calculate="string(/safe_pregnancy/household)"/>
		
		<bind nodeset="/safe_pregnancy/case/update/prp_edk" type="xsd:date" calculate="date(today() + (9 - /safe_pregnancy/months_pregnant) * 30.44)"/>
		<bind nodeset="/safe_pregnancy/case/update/prp_hrk" type="xsd:boolean" calculate="if(
																		   /safe_pregnancy/mother_age &gt;= 35 or 
																		   /safe_pregnancy/mother_age &lt;= 18 or 
																		   /safe_pregnancy/mother_height='under_150' or 
																		   (/safe_pregnancy/mother_education &lt;= 3 and /safe_pregnancy/partner_education &lt;=3) or 
																		   /safe_pregnancy/previous_csection='yes' or 
																		   /safe_pregnancy/previous_newborn_death='yes' or 
																		   /safe_pregnancy/previous_bleeding='yes' or 
																		   /safe_pregnancy/previous_terminations &gt;= 3 or 
																		   /safe_pregnancy/previous_pregnancies &gt;=5 or 
																		   /safe_pregnancy/heart_problems='yes' or 
																		   /safe_pregnancy/diabetes='yes' or 
																		   /safe_pregnancy/hip_problems='yes' or 
																		   /safe_pregnancy/hiv_result='positive' or 
																		   /safe_pregnancy/syphilis_result='positive' or 
																		   /safe_pregnancy/over_10_years='yes', 1, 0)"/>
		
		<!-- End Case XML Binds -->

		<bind id="household" nodeset="/safe_pregnancy/household"  type="xsd:int" required="true()" constraint=" .&gt;0 and .&lt;= 200" jr:constraintMsg="Namba ya kaya hiwezi kuwa 0 au zaidi ya 200?" />
		<bind id="phone" nodeset="/safe_pregnancy/phone" type="xsd:int" />
		<bind id="mother_name" nodeset="/safe_pregnancy/mother_name"  type="xsd:string" required="true()" />
		<bind id="mother_age" nodeset="/safe_pregnancy/mother_age"  type="xsd:int" required="true()" constraint=". &gt;= 14 and . &lt;= 60" jr:constraintMsg="Umri wa kuzaa ni kati ya 14 na 60" />
		<bind id="mother_education" nodeset="/safe_pregnancy/mother_education"  type="xsd:string" required="true()" />
		<bind id="mother_height" nodeset="/safe_pregnancy/mother_height"  type="xsd:string" required="true()" />
		<bind id="partner_name" nodeset="/safe_pregnancy/partner_name"  type="xsd:string" />
		<bind id="partner_age" nodeset="/safe_pregnancy/partner_age"  type="xsd:int" constraint=". &gt;= 14 and . &lt;= 70" jr:constraintMsg="Umri wa kuzaa ni kati ya 14 na 70" />
		<bind id="partner_education" nodeset="/safe_pregnancy/partner_education"  type="xsd:string" />
		<bind id="months_pregnant" nodeset="/safe_pregnancy/months_pregnant" type="xsd:int" required="true()" constraint=" .&lt; 12" jr:constraintMsg="Idadi ya miezi ya ujauzito haiwezi kuzidi 12"/>

		<bind id="previous_pregnancies" nodeset="/safe_pregnancy/previous_pregnancies"  type="xsd:int" required="true()" constraint=". &gt; 0 and  . &lt; 30" jr:constraintMsg="Number of Pregnancies Must Be At Least 1" />
		<bind id="previous_deliveries" nodeset="/safe_pregnancy/previous_deliveries"  type="xsd:int" required="true()" relevant="/safe_pregnancy/previous_pregnancies &gt; 1" constraint=" .&lt;/safe_pregnancy/previous_pregnancies" jr:constraintMsg="Idadi ya uzao haiwezi kuwa zaidi ya idadi ya mimba" />
		<bind id="previous_terminations" nodeset="/safe_pregnancy/previous_terminations"  type="xsd:int" required="true()" relevant="/safe_pregnancy/previous_pregnancies &gt; 1" constraint=" .&lt;/safe_pregnancy/previous_pregnancies" jr:constraintMsg="Angalia vizuri idadi ya kuharibisha mimba" />
		<bind id="children" nodeset="/safe_pregnancy/children"  type="xsd:int" required="true()" relevant="/safe_pregnancy/previous_pregnancies &gt; 1" constraint=" .&lt; 12" jr:constraintMsg="Angalia idadi ya watoto uliyoingiza"/>
		<bind id="previous_newborn_death" nodeset="/safe_pregnancy/previous_newborn_death"  type="xsd:string" required="true()" relevant="/safe_pregnancy/previous_pregnancies &gt; 1" />
		<bind id="over_10_years" nodeset="/safe_pregnancy/over_10_years"  type="xsd:string" required="true()" relevant="/safe_pregnancy/previous_pregnancies &gt; 1 and /safe_pregnancy/mother_age &gt; 24" />
		<bind id="previous_csection" nodeset="/safe_pregnancy/previous_csection"  type="xsd:string" required="true()" relevant="/safe_pregnancy/previous_pregnancies &gt; 1" />
		<bind id="previous_bleeding" nodeset="/safe_pregnancy/previous_bleeding"  type="xsd:string" required="true()" relevant="/safe_pregnancy/previous_pregnancies &gt; 1" />
		
		<bind id="heart_problems" nodeset="/safe_pregnancy/heart_problems"  type="xsd:string" required="true()" />
		<bind id="diabetes" nodeset="/safe_pregnancy/diabetes"  type="xsd:string" required="true()" />
		<bind id="hip_problems" nodeset="/safe_pregnancy/hip_problems"  type="xsd:string" required="true()" />

		<bind id="hiv_test" nodeset="/safe_pregnancy/hiv_test" type="xsd:string" required="true()" />
		<bind id="hiv_willing" nodeset="/safe_pregnancy/hiv_willing" type="xsd:string" relevant="/safe_pregnancy/hiv_test='yes'" />
		<bind id="hiv_result" nodeset="/safe_pregnancy/hiv_result" type="xsd:string" relevant="/safe_pregnancy/hiv_test='yes' and /safe_pregnancy/hiv_willing='yes'" />
		<bind nodeset="get_tested_info_hiv" relevant="../hiv_test = 'no'" />
		<bind id="syphilis_test" nodeset="/safe_pregnancy/syphilis_test" type="xsd:string" required="true()" />
		<bind id="syphilis_willing" nodeset="/safe_pregnancy/syphilis_willing" type="xsd:string" relevant="/safe_pregnancy/syphilis_test='yes'" />
		<bind id="syphilis_result" nodeset="/safe_pregnancy/syphilis_result" type="xsd:string" relevant="/safe_pregnancy/syphilis_test='yes' and /safe_pregnancy/syphilis_willing='yes'" />
		<bind nodeset="get_tested_info_syphilis" relevant="../syphilis_test = 'no'" />
				
		<bind nodeset="hi_risk_info" relevant="/safe_pregnancy/case/update/prp_hrk=1" />	
		
		<bind nodeset="hi_risk_info_old" relevant="../mother_age &gt;= 35" /> 
		<bind nodeset="hi_risk_info_young" relevant="../mother_age &lt;= 18" /> 
		<bind nodeset="hi_risk_info_education" relevant="../mother_education &lt;= 3 and ../partner_education &lt;=3" /> 
		<bind nodeset="hi_risk_info_small" relevant="../mother_height='under_150'" />		
		<bind nodeset="hi_risk_info_10_years" relevant="../over_10_years='yes'" />		
		<bind nodeset="hi_risk_info_complications" relevant="../previous_csection='yes' or ../previous_newborn_death='yes' or ../previous_bleeding='yes'" />
		<bind nodeset="hi_risk_info_many" relevant="../previous_terminations &gt;= 3 or ../previous_pregnancies &gt;=5" /> 
		<bind nodeset="hi_risk_info_health" relevant="../heart_problems='yes' or ../diabetes='yes' or ../hip_problems='yes'" />
		<bind nodeset="hi_risk_info_hiv" relevant="../hiv_result = 'positive'" />
		<bind nodeset="hi_risk_info_syphilis" relevant="../syphilis_result = 'positive'" />
	
		<bind id="more_details" nodeset="/safe_pregnancy/more_details"  type="xsd:string" />
	
</model>

